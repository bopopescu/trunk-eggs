<metal:block use-macro="here/help_index_html/macros/help_index">
	<metal:block fill-slot="main_content">
		<h2>User roles</h2>
		<dl>
			<dt>Viewer</dt>
				<dd>Can add comments</dd>
			<dt>Contributor</dt>
				<dd>Can add content</dd>
				<dd>Can delete own content</dd>
			<dt>Administrator</dt>
				<dd>Can administer Interest Group</dd>
			<dt>Manager</dt>
				<dd>Full access to management system</dd>
		</dl>
	</metal:block>
</metal:block>