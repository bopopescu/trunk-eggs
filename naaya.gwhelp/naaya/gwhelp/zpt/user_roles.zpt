<metal:block use-macro="here/gw_macro/macros/main">
	<div id="workarea" metal:fill-slot="div-workarea">
		<h1>User roles</h1>
		<dl>
			<dt>Viewer</dt>
				<dd>Can add comments</dd>
			<dt>Contributor</dt>
				<dd>Can add content</dd>
				<dd>Can delete own content</dd>
			<dt>Administrator</dt>
				<dd>Can administer Interest Group</dd>
			<dt>Manager</dt>
				<dd>Full access to management system</dd>
		</dl>
	</div>
</metal:block>