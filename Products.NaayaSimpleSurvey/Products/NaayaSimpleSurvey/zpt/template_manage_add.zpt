<html>
	<head>
		<title>Add Survey Template</title>
	</head>
	
	<body>
		<h1>Add Survey Template</h1>
		<form method="post" action="addNySurveyTemplate">
			<label for="id">Template id: </label>
			<input id="id" type="text" size="20" name="id" />
			<br />
			<label for="title">Template title</label>
			<input id="title" type="text" size="20" name="title" />
			<br />
			<label for="date">Template deadline (dd/mm/yyyy)</label>
			<input id="date" type="text" size="20" name="date" />
			<br />
			<input type="submit" value="Create Template" />
		</form>
	</body>
</html>