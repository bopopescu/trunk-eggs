        <td> <input type="text" id="datearri" name="datearri:utf8:ustring" size="20" tal:attributes="value python:here.getSession('datearri', '')" /></td>
 <th> <label i18n:translate="" for="dep_airport"> <b>Arriving from</b> </label></th>
        <td> <input type="text" id="dep_airport" name="dep_airport:utf8:ustring" size="20" tal:attributes="value python:here.getSession('dep_airport', '')" /></td>
        <th> <label i18n:translate="" for="flight"> <b> Flight number</b> </label></th>
        <td> <input type="text" id="flight_numberarri" name="flight_numberarri:utf8:ustring" size="15" tal:attributes="value python:here.getSession('flight_numberarri', '')" /></td>
        <th> <label i18n:translate="" for="flight"><b> Time of arrival</b></label></th>
        <td> <input type="text" id="timearri" name="timearri:utf8:ustring" size="15" tal:attributes="value python:here.getSession('timearri', '')" /></td>
        </tr>

        <tr>
        <th> <label i18n:translate="" for="datedepar"> <b>Date of departure</b> </label></th>
        <td> <input type="text" id="datedepar" name="datedepar:utf8:ustring" size="20" tal:attributes="value python:here.getSession('datedepar', '')" /></td>
        <th> <label i18n:translate="" for="flight"> <b> Flight number:</b> </label></th>
        <td> <input type="text" id="flight_numberdepar" name="flight_numberdepar:utf8:ustring" size="15" tal:attributes="value python:here.getSession('flight_numberdepar', '')" /></td>
        <th> <label i18n:translate="" for="flight"> <b> Time of departure</b></label></th>
        <td> <input type="text" id="timepar" name="timepar:utf8:ustring" size="15" tal:attributes="value python:here.getSession('timepar', '')" /></td>
        </tr>
       </table>


